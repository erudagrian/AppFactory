    "@ngrx/router-store": "4.1.1",
    "@ngrx/store": "4.1.1",
    "@ngrx/store-devtools": "4.0.0",
    "@nrwl/nx": "0.7.4",
    "angularfire2": "^5.0.0-rc.6",
    "bootstrap": "^4.0.0",
    "core-js": "^2.4.1",
    "firebase": "^4.10.1",
    "firebase-tools": "^3.17.4",
    "ngx-bootstrap": "^2.0.2",
    "rxjs": "^5.5.6",
    "zone.js": "^0.8.19"
